    "js-yaml": "^3.14.1",
    "serialize-error": "^7.0.1"
  },
  "devDependencies": {
    "@types/async": "^3.2.5",
    "@types/byline": "^4.2.31",
    "@types/js-yaml": "^3.12.6",
    "@types/node": "^10.17.54",
    "@typescript-eslint/eslint-plugin": "^4.15.1",
    "@typescript-eslint/parser": "^4.15.1",
    "eslint": "^7.20.0",
    "mkdirp": "^1.0.4",
    "ts-node": "^8.10.2",
    "typescript": "^4.1.5"
  },
  "scripts": {
    "lint": "eslint ./src --ext .ts,.tsx",
