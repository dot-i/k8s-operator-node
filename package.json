  },
  "devDependencies": {
    "@types/async": "^3.2.16",
    "@types/byline": "^4.2.33",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "=12.12.6",
    "@types/request": "^2.48.8",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.48.2",
    "@typescript-eslint/parser": "^5.48.2",
    "eslint": "^8.32.0",
    "mkdirp": "^1.0.4",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "scripts": {
    "lint": "eslint ./src --ext .ts,.tsx",
