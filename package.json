{
  "name": "@dot-i/k8s-operator",
  "version": "1.3.1",
  "description": "Build Kubernetes operators in NodeJS (and TypeScript)",
  "author": {
    "name": "Nico Francois",
    "email": "nico.francois@dot-i.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dot-i/k8s-operator-node"
  },
  "files": [
    "dist/*.ts",
    "dist/*.js",
    "*.md"
  ],
  "license": "Apache-2.0",
  "main": "dist/operator.js",
  "types": "dist/operator.d.ts",
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "@kubernetes/client-node": "^0.16.3",
    "async": "^3.2.4",
    "gaxios": "^5.0.0"
  },
  "devDependencies": {
    "@types/async": "^3.2.13",
    "@types/byline": "^4.2.33",
    "@types/node": "^12.12.6",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "eslint": "^8.18.0",
    "mkdirp": "^1.0.4",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4"
  },
  "scripts": {
    "lint": "eslint ./src --ext .ts,.tsx",
    "clean": "rm -Rf node_modules/ dist/",
    "build": "tsc",
